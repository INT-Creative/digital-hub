            <span class="inline-block bg-sage-100 text-sage-800 px-2 py-1 rounded text-xs font-medium">Sales Enablement</span>
          </div>
        </div>

        <!-- Case Study 4 -->
        <div class="bg-white rounded-xl p-8 shadow-sm hover:shadow-md transition-all duration-200">
          <div class="flex items-center mb-4">
            <div class="w-12 h-12 bg-gradient-to-r from-purple-500 to-pink-600 rounded-lg flex items-center justify-center text-white font-bold">
              RL
            </div>
            <div class="ml-3">
              <h3 class="text-lg font-semibold text-forest-900">RetailLogic</h3>
