{
  "name": "intcreative-hub",
  "type": "module",
  "version": "1.0.0",
  "description": "Professional marketing agency website for startup-focused lead generation",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "format": "prettier --write .",
    "check": "astro check",
    "test": "jest --config=tests/setup/jest.config.js",
    "test:watch": "jest --config=tests/setup/jest.config.js --watch",
    "test:coverage": "jest --config=tests/setup/jest.config.js --coverage",
    "test:unit": "jest --config=tests/setup/jest.config.js tests/unit",
    "test:integration": "jest --config=tests/setup/jest.config.js tests/integration",
    "test:performance": "jest --config=tests/setup/jest.config.js tests/performance",
    "test:accessibility": "jest --config=tests/setup/jest.config.js tests/accessibility",
    "test:seo": "jest --config=tests/setup/jest.config.js tests/seo",
    "test:legal": "jest --config=tests/setup/jest.config.js tests/legal",
    "test:e2e": "playwright test --config=tests/setup/playwright.config.js",
    "test:e2e:ui": "playwright test --config=tests/setup/playwright.config.js --ui",
    "test:e2e:headed": "playwright test --config=tests/setup/playwright.config.js --headed",
    "test:e2e:debug": "playwright test --config=tests/setup/playwright.config.js --debug",
    "test:cross-browser": "playwright test --config=tests/setup/playwright.config.js --project=chromium-desktop --project=firefox-desktop --project=webkit-desktop",
    "test:mobile": "playwright test --config=tests/setup/playwright.config.js --project=mobile-chrome --project=mobile-safari",
    "test:accessibility-e2e": "playwright test --config=tests/setup/playwright.config.js --project=accessibility-audit",
    "test:performance-e2e": "playwright test --config=tests/setup/playwright.config.js --project=performance-audit",
    "test:legal-e2e": "playwright test --config=tests/setup/playwright.config.js --project=legal-compliance",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "license:check": "node scripts/license-audit.js",
    "license:audit": "node scripts/license-audit.js --report",
    "license:fix": "node scripts/license-audit.js --fix",
    "license:scan": "npx license-checker --onlyAllow 'MIT;Apache-2.0;ISC;BSD-2-Clause;BSD-3-Clause'",
    "license:report": "npx license-checker --json > license-report.json"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "astro": "^5.13.5",
    "license-checker": "^25.0.1",
    "noop": "^1.0.1",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.31.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.0",
    "prettier": "^3.2.0",
    "prettier-plugin-astro": "^0.13.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "marketing",
    "startup",
    "lead-generation",
    "astro",
    "static-site"
  ],
  "author": "INT Creative LLC <jake@intcreative.co>",
  "license": "UNLICENSED",
  "homepage": "https://intcreative.co",
  "repository": {
    "type": "git",
    "url": "https://github.com/intcreative/intcreative-hub.git"
  },
  "bugs": {
    "url": "https://github.com/intcreative/intcreative-hub/issues"
  },
  "overrides": {
    "sharp": "npm:noop@^1.0.0"
  }
}
